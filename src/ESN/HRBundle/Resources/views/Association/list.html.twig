<div class="row">

                        <div class="span5">
                                <div class="widget">
                                    <div class="widget-content">
                                    
                                        <a class="btn" href="{{ path('esn_hr_association_add') }}">Add new Esner</a>

                                    </div><!-- /widget-content --> 
                                </div><!-- /widget -->    
                            </div>

                    <div class="span12">
                        <div class="widget">
                            <div class="widget-content">
                                              
                                <div class="panel panel-default">

                                    <!-- Table -->
                                    <table class="table">
                                      <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th>Surname</th>
                                            <th>Admission Date</th>
                                            <th>Action</th>
                                        </tr>
                                      </thead>
                                      <tbody>
                                          
                                          {% for esner in esners %}
                                                <tr>
                                                    <td>{{esner.member.name}}</td>
                                                    <td>{{esner.member.surname}}</td>
                                                    <td>{{esner.member.inscription|date('Y-m-d')}}</td>
                                                    <td>
                                                        <a href="{{ path('esn_hr_association_edit', {'id' : esner.member.id}) }}">Edit</a> &nbsp;
                                                        <a href="{{ path('esn_hr_association_delete', {'id' : esner.member.id}) }}">Delete</a>
                                                    </td>
                                                </tr>
                                          {% endfor %} 
                                            
                                      </tbody>
                                    </table>
                                </div>
                            </div><!-- widget-content -->
                        </div><!-- /widget -->
                    </div><!-- /span12 -->   
        
                    </div>
