{% extends '::base.html.twig' %}


{% block subnavbar %}
    {{ render(controller("ESNDashboardBundle:Dashboard:subnavbar")) }}
{% endblock %}

{% block body %}
    <div class="span12">
        <p>
            {% for message in app.session.flashbag.get('notice') %}
                <div class="alert alert-success">
                    <a href="#" class="close" data-dismiss="alert">&times;</a>
                    {{ message }}
                </div>
            {% endfor %}
        </p>
    </div>
{% endblock %}
